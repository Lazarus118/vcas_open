@model VCAS.Models.VCAS_issuedChecks
@{
    ViewBag.Title = "Delete";
}


<div class="form-horizontal">
    <div class="grid" style="background: #FFFFFF; border-radius: 10px; width: 100%; padding: 20px;">
        <div class="row">
            <div class="cell-12">
                <h2>Delete</h2>
                <h3>Are you sure you want to delete this?</h3>
                <p>Expense: ID # @Model.Id</p>
                <p>For: @Model.receiverName</p>
                <hr />
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div>
                        <input type="submit" value="Delete" class="button alert" style="width: 100%;" />
                    </div>
                }
            </div>
        </div>
    </div>
</div>

